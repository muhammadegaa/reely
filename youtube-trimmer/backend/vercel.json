{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "PYTHONPATH": "/var/task"
  },
  "functions": {
    "api/index.py": {
      "maxDuration": 300
    }
  },
  "regions": [
    "iad1"
  ],
  "outputDirectory": ".vercel/output",
  "installCommand": "pip install -r requirements-vercel.txt",
  "buildCommand": "echo 'Build completed'",
  "devCommand": "uvicorn main_vercel:app --host 0.0.0.0 --port 3000 --reload"
}